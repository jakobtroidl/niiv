    "version": "0.1.0",
    "configurations": [
        {
            "name": "Create Dataset",
            "type": "python",
            "request": "launch",
            "module": "experiments.create_dataset",
            "justMyCode": true,
            "args": [
                "--name=hemibrain-volume-noisy-large",
                "--path=https://storage.googleapis.com/neuroglancer-janelia-flyem-hemibrain/emdata/clahe_yz/jpeg",
                "--image_size=128",
                "--n_images=400",
                "--train",
                "--anistropic_dim=2",
            ]
        },
        {
            "name": "Create Test Sequence",
            "type": "python",
            "request": "launch",
            "module": "experiments.create_test_sequence",
            "justMyCode": true,
            "args": [
                "--name=hemibrain-volume-noisy-test-sequence",
                "--path=https://storage.googleapis.com/neuroglancer-janelia-flyem-hemibrain/emdata/clahe_yz/jpeg",
                "--mip=0",
                "--test_final",
            ]
        },
        {
            "name": "Train Images",
            "type": "python",
            "request": "launch",
            "module": "experiments.train_images",
            "justMyCode": true,
            "args": [
                "--experiment_name=hemibrain-volume-noisy-loss-ablation",
                "--dataset=data/hemibrain-volume-noisy-large/info.json",
                "--config=./config/config_cvr_s.json"
            ]
        },
        {
            "name": "Eval Sequences",
            "type": "python",
            "request": "launch",
            "module": "experiments.eval_seq_simplified",
            "justMyCode": true,
            "args": [
                "--experiment_name=hemibrain-volume-noisy-loss-ablation",
                "--dataset=data/hemibrain-volume-noisy-large/info.json",
                "--config=./config/config_cvr_s.json",
                "--iteration=70"
            ]
        }
    ]
}