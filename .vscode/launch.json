    "version": "0.1.0",
    "configurations": [
        {
            "name": "Create 2D Dataset",
            "type": "python",
            "request": "launch",
            "module": "experiment_scripts.create_dataset",
            "justMyCode": true,
            "args": [
                "--name=hemibrain-denoised-large-mip-0",
                "--path=https://storage.googleapis.com/neuroglancer-janelia-flyem-hemibrain/emdata/clahe_yz/jpeg",
                "--image_size=128",
                "--n_images=2000",
                "--train",
                "--anistropic_dim=2",
                "--denoise"

            ]
        },
        {
            "name": "Create Test Sequence",
            "type": "python",
            "request": "launch",
            "module": "experiment_scripts.create_test_sequence",
            "justMyCode": true,
            "args": [
                "--name=hemibrain-denoised-large-mip-0",
                "--path=https://storage.googleapis.com/neuroglancer-janelia-flyem-hemibrain/emdata/clahe_yz/jpeg",
                "--mip=0",
                "--denoise",
            ]
        },
        {
            "name": "Train Images",
            "type": "python",
            "request": "launch",
            "module": "experiment_scripts.train_images",
            "justMyCode": true,
            "args": [
                "--experiment_name=images-hemibrain-large-denoised",
                "--dataset=data/hemibrain-denoised-large-mip-0/info.json",
                "--config=./config/config_cvr_s.json"
            ]
        },
        {
            "name": "Eval Images",
            "type": "python",
            "request": "launch",
            "module": "experiment_scripts.eval_images",
            "justMyCode": true,
            "args": [
                "--experiment_name=images-h01-larger-data",
                "--dataset=data/minnen-denoised-mip-0-small-images-large/info.json",
                "--config=./config/config_cvr_s.json",
            ]
        },
        {
            "name": "Eval  Sequences",
            "type": "python",
            "request": "launch",
            "module": "experiment_scripts.eval_image_sequences",
            "justMyCode": true,
            "args": [
                "--experiment_name=images-hemibrain-large-denoised",
                "--dataset=data/hemibrain-denoised-large-mip-0/info.json",
                "--config=./config/config_cvr_s.json",
            ]
        }
    ]
}